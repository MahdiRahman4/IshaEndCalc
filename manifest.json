{
    "manifest_version": 3,
    "name":"Isha End Time",
    "version": "1.0.1",
    "description": "An extension made for calculating the end of the Isha prayer.",
    "icons": {
        "48" : "logo.png",
        "128": "logo.png"
    },
   "permissions": ["geolocation"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "logo.png"
        }
    }
}